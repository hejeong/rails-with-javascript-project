<%= form_for @user, controller: 'users', action: 'create' do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%> <br> <br>
    <%= f.label :username %>
    <%= f.text_field :username%> <br> <br>
    <%= f.label :password %>
    <%= f.text_field :password%> <br> <br>
    <%= f.label :email %>
    <%= f.text_field :email%> 
    <%= f.submit 'Submit'%>
<% end %>